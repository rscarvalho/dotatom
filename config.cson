"*":
  "MavensMate-Atom":
    mm_panel_height: 275
  Termrk:
    fontFamily: "Source Code Pro for Powerline"
  "atom-material-ui":
    accentColor: "Orange"
    altCmdPalette: true
    compactTreeView: true
    depth: true
    panelContrast: true
    showTabIcons: "Show on all tabs"
    tabSize: "Small"
  core:
    disabledPackages: [
      "language-babel"
      "language-jsx"
      "tool-bar"
      "vim-mode-plus"
    ]
    themes: [
      "one-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  editor:
    fontFamily: "Menlo"
    fontSize: 15
    invisibles: {}
    lineHeight: 1.3
    scrollPastEnd: true
    showIndentGuide: true
    tabType: "soft"
  "exception-reporting":
    userId: "ef6dbf64-d6d6-089b-4fb5-c0247d65b442"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "flex-tool-bar": {}
  "git-plus": {}
  "highlight-line":
    hideHighlightOnSelect: true
  "highlight-selected":
    highlightBackground: true
    lightTheme: true
    onlyHighlightWholeWords: true
  "isotope-ui": {}
  "language-babel":
    transpileOnSave: false
  linter:
    errorPanelHeight: 109
    showErrorTabLine: true
    statusBar: "Show error of the selected line"
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
    globalNodePath: "/opt/boxen/homebrew"
    showRuleIdInMessage: true
    useGlobalEslint: true
  "merge-conflicts": {}
  nuclide:
    remoteProjectsConfig: []
  react:
    enabledForAllJavascriptFiles: true
  "tool-bar":
    position: "Left"
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
  "vim-mode-plus":
    incrementalSearch: true
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
